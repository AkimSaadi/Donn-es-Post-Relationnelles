<!ELEMENT Recettes (recette | auteur | categoriePlat | sousCategoriePlat | produitIngredient | ingredient)*><!ELEMENT recette (titre, resumé?, photo, datePublication, difficulté, ingredients, nombrePersonne, tempsPréparation, tempsCuisson, tempsRepos, description?, note, ref-ingredient*, ref-sous-categorie, ref-auteur*)><!ELEMENT resumé (p)><!ELEMENT titre (#PCDATA)><!ELEMENT photo (#PCDATA)><!ELEMENT datePublication (#PCDATA)><!ELEMENT difficulté (#PCDATA)><!ELEMENT ingredients (ingredientRecette)*><!ELEMENT ingredientRecette (ref | quantite)*><!ELEMENT ref (#PCDATA)><!ELEMENT quantite (#PCDATA)><!ELEMENT ingredient (nom, apportNutritif, apportEnergetique, saison, description?, ref-recette*)><!ELEMENT nom (#PCDATA)><!ELEMENT apportNutritif (#PCDATA)><!ELEMENT apportEnergetique (#PCDATA)><!ELEMENT saison (#PCDATA)><!ELEMENT nombrePersonne (#PCDATA)><!ELEMENT tempsPréparation (#PCDATA)><!ELEMENT tempsCuisson (#PCDATA)><!ELEMENT tempsRepos (#PCDATA)><!ELEMENT description (p)><!ELEMENT p (#PCDATA | p | t | br)*><!ELEMENT t (#PCDATA)><!ELEMENT br EMPTY><!ELEMENT note (#PCDATA)><!ELEMENT auteur (nom, age, pays, sexe, biographie, ref-recette*)><!ELEMENT age (#PCDATA)><!ELEMENT pays (#PCDATA)><!ELEMENT sexe (#PCDATA)><!ELEMENT biographie (#PCDATA)><!ELEMENT ref-auteur EMPTY><!ELEMENT ref-recette EMPTY><!ELEMENT ref-ingredient EMPTY><!ELEMENT categoriePlat (nom, description)><!ELEMENT sousCategoriePlat (nom, description, nombreRecette, ref-categorie)><!ELEMENT nombreRecette (#PCDATA)><!ELEMENT produitIngredient (nom, ref-ingredient+)><!ELEMENT ref-categorie EMPTY><!ELEMENT ref-sous-categorie EMPTY><!ATTLIST recette	id ID #REQUIRED><!ATTLIST ingredient	id ID #REQUIRED><!ATTLIST produitIngredient	id ID #REQUIRED><!ATTLIST auteur	id ID #REQUIRED><!ATTLIST sousCategoriePlat	id ID #REQUIRED><!ATTLIST ref-auteur	ref IDREF #REQUIRED><!ATTLIST ref-recette	ref IDREF #REQUIRED><!ATTLIST ref-ingredient	ref IDREF #REQUIRED><!ATTLIST ref-sous-categorie	ref IDREF #REQUIRED><!ATTLIST categoriePlat	id ID #REQUIRED><!ATTLIST ref-categorie	ref IDREF #REQUIRED>